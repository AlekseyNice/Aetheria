# Порт сервера
PORT=3000

# Окружение (development, production)
NODE_ENV=development

# Настройки базы данных
DB_PATH=./data/servers.db

# Настройки SSH
SSH_TIMEOUT=10000
SSH_MAX_CONCURRENT=5

# Настройки безопасности
ENABLE_AUTH=false
AUTH_USERNAME=admin
AUTH_PASSWORD=your_secure_password_here

# Логирование
LOG_LEVEL=info
LOG_FILE=./logs/aetheria.log